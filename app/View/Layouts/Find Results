  551:  * Tries using the standard XmlHttpRequest, then IE's wacky ActiveX Objects.
  552   */
  553  DEBUGKIT.Util.Request.prototype.createObj = function () {
  554  	var request = null;
  555  	try {
  556: 		request = new XMLHttpRequest();
  557  	} catch (MS) {
  558  		try {
  559: 			request = new ActiveXObject('Msxml2.XMLHTTP');
  560  		} catch (old_MS) {
  561  			try {
  562: 				request = new ActiveXObject('Microsoft.XMLHTTP');
  563  			} catch (failure) {
  564  				request = null;

