  157  	public function testIsOk() {
  158: 		$this->HttpResponse->code = 0;
  159: 		$this->assertFalse($this->HttpResponse->isOk());
  160: 		$this->HttpResponse->code = -1;
  161: 		$this->assertFalse($this->HttpResponse->isOk());
  162: 		$this->HttpResponse->code = 'what?';
  163: 		$this->assertFalse($this->HttpResponse->isOk());
  164: 		$this->HttpResponse->code = 200;
  165: 		$this->assertTrue($this->HttpResponse->isOk());
  166: 		$this->HttpResponse->code = 201;
  167: 		$this->assertTrue($this->HttpResponse->isOk());
  168: 		$this->HttpResponse->code = 202;
  169: 		$this->assertTrue($this->HttpResponse->isOk());
  170: 		$this->HttpResponse->code = 203;
  171: 		$this->assertTrue($this->HttpResponse->isOk());
  172: 		$this->HttpResponse->code = 204;
  173: 		$this->assertTrue($this->HttpResponse->isOk());
  174: 		$this->HttpResponse->code = 205;
  175: 		$this->assertTrue($this->HttpResponse->isOk());
  176: 		$this->HttpResponse->code = 206;
  177: 		$this->assertTrue($this->HttpResponse->isOk());
